        fx:controller="com.baro.controllers.orderDetail.OrderDetailExtraController"
        xmlns="http://javafx.com/javafx/8.0.291"
        xmlns:fx="http://javafx.com/fxml/1" style="-fx-background-color: white"
>
    <Label fx:id="extraNameLabel" text="엑스트라" style="-fx-font-size: 11pt; -fx-text-fill: #787878" GridPane.rowIndex="0"
           GridPane.columnIndex="0"/>
    <Label fx:id="extraCountLabel" text="개수" style="-fx-font-size: 11pt; -fx-text-fill: #787878" GridPane.rowIndex="0"
           GridPane.columnIndex="1"/>
    <Label fx:id="extraPriceLabel" text="가격" style="-fx-font-size: 11pt; -fx-text-fill: #787878" GridPane.rowIndex="0"
           GridPane.columnIndex="2"/>
</GridPane>
